{
  "canisters": {
    "api": {
      "main": "canisters/api/main.mo",
      "type": "motoko"
    },
    "storage": {
      "main": "canisters/storage/main.mo",
      "type": "motoko",
      "dependencies": [
        "api"
      ]
    },
    "frontend": {
      "dependencies": [
        "api"
      ],
      "source": [
        "frontend/dist"
      ],
      "build": [
        "npm run build --workspaces --if-present"
      ],
      "type": "assets",
      "workspace": "frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "cat mops.sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}